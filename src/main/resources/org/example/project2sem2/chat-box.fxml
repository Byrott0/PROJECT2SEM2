<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="parentID" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.project2sem2.Controller.ChatBoxController">
   <children>
      <Label fx:id="NameID" layoutX="93.0" layoutY="10.0" AnchorPane.leftAnchor="93.0" AnchorPane.topAnchor="10.0">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Label>

      <ListView fx:id="listviewID" prefWidth="106.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="71.0" />

      <Button fx:id="chatlistID" onAction="#addChat" prefWidth="106.0" text="Voeg Chat" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="45.0" />

      <TextField fx:id="textfield" prefHeight="34.0" prefWidth="650.0" promptText="Stel uw vraag" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="30.0">
         <font>
            <Font size="18.0" />
         </font></TextField>

      <Button fx:id="textfieldEnterID" layoutX="737.0" layoutY="553.0" minHeight="15.0" minWidth="15.0" onAction="#checkText" prefHeight="26.0" prefWidth="27.0" text="&gt;" AnchorPane.bottomAnchor="21.0" AnchorPane.rightAnchor="36.0" />

      <TextArea fx:id="textAreaID" editable="false" promptText="Hier komt uw gesprek te staan." AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="69.0">
         <font>
            <Font name="System Bold" size="19.0" />
         </font></TextArea>

      <MenuButton fx:id="instellingenID" layoutX="636.0" layoutY="16.0" minWidth="80.0" prefHeight="38.0" prefWidth="134.0" text="Instellingen" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="16.0">
         <items>
            <MenuItem fx:id="adminID" mnemonicParsing="false" onAction="#goToAdminPage" text="Admin" />
            <Menu fx:id="languageMenuID" mnemonicParsing="false" text="Taal">
               <items>
                  <MenuItem fx:id="dutchID" mnemonicParsing="false" onAction="#setDutch" text="Nederlands" />
                  <MenuItem fx:id="englishID" mnemonicParsing="false" onAction="#setEnglish" text="English" />
               </items>
            </Menu>
            <MenuItem fx:id="uitloggenID" mnemonicParsing="false" onAction="#LogOutfunction" text="Uitloggen" />
         </items>
         <font>
            <Font size="16.0" />
         </font>
      </MenuButton>
   </children>
</AnchorPane>
